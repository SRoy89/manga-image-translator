{
  "translator": {
    "translator": "deepl"
  },
  "detector": {
    "detector": "ctd",
    "detection_size": 2048,
    "text_threshold": 0.45,
    "box_threshold": 0.74,
    "det_rotate": true,
    "det_auto_rotate": true,
    "det_invert": true,
    "det_gamma_correct": true
  },
  "ocr": {
    "text_scale": 32,
    "use_mocr_merge": true,
    "min_text_length": 0,
    "ignore_bubble": 0
  },
  "inpainter": {
    "inpainter": "lama_large",
    "inpainting_size": 2048,
    "inpainting_precision": "bf16"
  },
  "render": {
    "renderer": "manga2eng",
    "alignment": "auto",
    "disable_font_border": false,
    "font_size_offset": 0,
    "font_size_minimum": -1
  }
}